{
  "name": "mindforgex-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.14.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "seed-counters": "node -r tsconfig-paths/register -r ts-node/register src/command/databases/seeds/counters/seed.ts",
    "seed-channels": "node -r tsconfig-paths/register -r ts-node/register src/command/databases/seeds/channels/seed.ts",
    "update-channels": "node -r tsconfig-paths/register -r ts-node/register src/command/databases/seeds/channels/update.ts",
    "seed-nft": "node -r tsconfig-paths/register -r ts-node/register src/command/databases/seeds/nft/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.360.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@solana/spl-account-compression": "^0.1.10",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.78.5",
    "axios": "^1.5.0",
    "bs58": "^5.0.0",
    "cache-manager": "^5.2.3",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^11.0.0",
    "decimal.js": "^10.4.3",
    "helmet": "^7.0.0",
    "mongoose": "^7.3.1",
    "mongoose-autopopulate": "^1.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tweetnacl": "^1.0.3",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
